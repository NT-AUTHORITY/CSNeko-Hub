#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/x/CSNeko-Hub/node_modules/.pnpm/vitepress@1.6.3_@algolia+cl_cb10b30a29476d1fc90ffe0499a9958b/node_modules/vitepress/bin/node_modules:/mnt/x/CSNeko-Hub/node_modules/.pnpm/vitepress@1.6.3_@algolia+cl_cb10b30a29476d1fc90ffe0499a9958b/node_modules/vitepress/node_modules:/mnt/x/CSNeko-Hub/node_modules/.pnpm/vitepress@1.6.3_@algolia+cl_cb10b30a29476d1fc90ffe0499a9958b/node_modules:/mnt/x/CSNeko-Hub/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/x/CSNeko-Hub/node_modules/.pnpm/vitepress@1.6.3_@algolia+cl_cb10b30a29476d1fc90ffe0499a9958b/node_modules/vitepress/bin/node_modules:/mnt/x/CSNeko-Hub/node_modules/.pnpm/vitepress@1.6.3_@algolia+cl_cb10b30a29476d1fc90ffe0499a9958b/node_modules/vitepress/node_modules:/mnt/x/CSNeko-Hub/node_modules/.pnpm/vitepress@1.6.3_@algolia+cl_cb10b30a29476d1fc90ffe0499a9958b/node_modules:/mnt/x/CSNeko-Hub/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/vitepress@1.6.3_@algolia+cl_cb10b30a29476d1fc90ffe0499a9958b/node_modules/vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../.pnpm/vitepress@1.6.3_@algolia+cl_cb10b30a29476d1fc90ffe0499a9958b/node_modules/vitepress/bin/vitepress.js" "$@"
fi
